{
    "version": 2,
    "builds": [
      {
        "src": "./Backend/node.js",  
        "use": "@vercel/node"
      },
      {
        "src": "./build",    
        "use": "@vercel/static"
      }
    ],
  
    
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/Backend/node.js"
      }
    ]
  }